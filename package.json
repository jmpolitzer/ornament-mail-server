{
  "name": "ornament-web-boilerplate",
  "version": "1.0.0",
  "description": "Spin up web apps in no time.",
  "main": "server.js",
  "scripts": {
    "test": "npm test",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "babel-watch server/index.js",
    "client": "node start-client.js",
    "build": "babel server -d dist",
    "start": "node dist/index.js"
  },
  "babel": {
    "presets": [
      "env",
      "stage-2"
    ]
  },
  "author": "Jay Politzer",
  "license": "ISC",
  "dependencies": {
    "express": "^4.15.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.7",
    "concurrently": "^3.5.0"
  }
}
